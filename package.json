{
  "name": "recipe_api",
  "description": "Node RESTful API",
  "private": true,
  "scripts": {
    "start": "set NODE_ENV=production&&node ./bin/www.js",
    "start:dev": "set NODE_ENV=development&&node --inspect ./bin/www.js",
    "test:class": "asmine \"spec/class/*.spec.js\"",
    "test:server": "set NODE_ENV=development&&jasmine \"spec/api/*.spec.js\""
  },
  "engines": {
    "node": "10.*"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sberlinches/recipe_api.git"
  },
  "dependencies": {
    "express": "4.16.3",
    "body-parser": "1.18.3",
    "cors": "2.8.4",
    "compression": "1.7.2",
    "jasmine": "3.1.0",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "request": "2.87.0"
  }
}
